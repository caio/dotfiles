XTerm*color3: #f7e78c
XTerm*color4: #84cfef
XTerm*color5: #ce5d5a
XTerm*color6: #87ff87

xterm*faceName: Terminus
xterm*faceSize: 8

XTerm*geometry: 80x20
XTerm*scrollBar: false

! urxvt

Xft.antialias: true
urxvt*geometry: 80x20
!urxvt*inheritPixmap:     true
!urxvt*reverseVideo: false
urxvt*tintColor: #dfdfdf
!urxvt*shading:         20  !trans, 0=opaque    
!urxvt*fading:         10  !term unfocus      
urxvt.font xft:Terminus-8
urxvt.perl-ext-common: default,matcher
urxvt*scrollBar: false
urxvt*scrollBar_floating: true
urxvt*scrollBar_right: false
urxvt*scrollColor:  #000000
urxvt*urllauncher: firefox
urxvt*matcher.button: 3
urxvt*saveLines: 8192
urxvt*termName:  xterm
urxvt*cursorColor:  #d8d8d8
urxvt*colorBD:  lightyellow
urxvt*colorUL:  yellow

