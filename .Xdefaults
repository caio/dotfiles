! Global
Xft.antialias: true
Xft.rgba: rgb
Xft.hintstyle: hintfull
Xft.hinting: true

! Xterm

xterm*underLine: on
xterm*colorMode: on
xterm*colorBD: lightyellow
!xterm*colorBDMode: off
xterm*colorUL: yellow
xterm*colorULMode: on
xterm*dynamicColors: off
xterm*highlightSelection: true
xterm*faceName: Terminus
xterm*faceSize: 8
xterm*geometry: 80x20
xterm*scrollBar: false

xterm*background : #2e3436
xterm*foreground : #ffffff
! Black
xterm*color8     : #888a85
xterm*color0     : #2e3436
! Red
xterm*color9     : #ef2828
xterm*color1     : #a40000
! Green
xterm*color10   : #8ae234
xterm*color2    : #4e9a06
! Yellow
xterm*color11   : #fce94f
xterm*color3    : #c4a000
! Blue
xterm*color12   : #729fcf
xterm*color4    : #204a87
! Purple
xterm*color13   : #ad7fa8
xterm*color5    : #5c3566
! Cyan
xterm*color14   : #92B19E
xterm*color6    : #A1CDCD
! White
xterm*color15   : #eeeeec
xterm*color7    : #babdb6


! urxvt

!urxvt*termName:  xterm
urxvt*xftAntialias: true
urxvt*background : #2e3436
urxvt*foreground : #ffffff
urxvt*geometry: 80x20
urxvt*tintColor: #dfdfdf
urxvt*font: xft:Envy\ Code\ R:pixelsize=14
urxvt.perl-ext-common: default,matcher
urxvt*scrollBar: false
urxvt*scrollBar_floating: true
urxvt*scrollBar_right: false
urxvt*scrollColor:  #000000
urxvt*urllauncher: firefox
urxvt*matcher.button: 3
urxvt*saveLines: 8192
urxvt*cursorColor:  #d8d8d8
urxvt*colorBD:  lightyellow
urxvt*colorUL:  yellow

!colors
! Black
urxvt*color8     : #888a85
urxvt*color0     : #2e3436
! Red
urxvt*color9     : #ef2828
urxvt*color1     : #a40000
! Green
urxvt*color10   : #8ae234
urxvt*color2    : #4e9a06
! Yellow
urxvt*color11   : #fce94f
urxvt*color3    : #c4a000
! Blue
urxvt*color12   : #729fcf
urxvt*color4    : #204a87
! Purple
urxvt*color13   : #ad7fa8
urxvt*color5    : #5c3566
! Cyan
urxvt*color14   : #92B19E
urxvt*color6    : #A1CDCD
! White
urxvt*color15   : #eeeeec
urxvt*color7    : #babdb6

! NEDIT

nedit*enableThinThickness:     True
nedit*enableToggleVisual:      True
nedit*enableToggleColor:       True
nedit*XmToggleButton.indicatorSize: 11
nedit*enableEtchedInMenu:      True
nedit*XmMenuShell*XmTearOffButton*ShadowThickness:   2
nedit*text.background:          white
nedit*text.selectBackground:   #D0D0FF
nedit*text.lineNumForeground:  #A0A0D0
nedit*background:              gray85
nedit*statsForm.background:    gray85
nedit*statsLine.background:    gray85
nedit*bottomShadowColor:       gray65
nedit*topShadowColor:          #fefefe
nedit*XmText.background:       white
nedit*XmTextField.background:  white
nedit*XmList.background:       white
